{
  "name": "vue-toastify",
  "version": "1.8.0",
  "private": false,
  "description": "Simple, extendable, dependency free notification plugin.",
  "main": "./dist/vue-toastify.umd.min.js",
  "scripts": {
    "serve": "serve -s build",
    "watch": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "compile": "cross-env IS_COMPILE=true vue-cli-service build --target lib --dest dist/ --name vue-toastify ./src/VueToastify.js",
    "test": "jest --config=./jest.config.js --watch"
  },
  "sideEffects": true,
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.30",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^24.9.0",
    "node-sass": "^4.14.1",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.0",
    "tailwindcss": "^1.4.4",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11",
    "webpack-build-notifier": "^2.0.0"
  },
  "keywords": [
    "vue",
    "notification",
    "toastify",
    "laravel",
    "pause",
    "alert",
    "toast",
    "status",
    "notification-plugin",
    "pause",
    "draggable",
    "customisable",
    "extendable"
  ],
  "files": [
    "dist/*"
  ],
  "author": "Nandor Kraszlan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nandi95/vue-toastify/issues"
  },
  "homepage": "https://github.com/nandi95/vue-toastify#readme"
}
